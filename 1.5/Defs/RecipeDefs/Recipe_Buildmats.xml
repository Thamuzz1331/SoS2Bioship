<Defs>
	<RecipeDef>
		<defName>MakeProteinCLay</defName>
		<label>make protein clay</label>
		<description>Craft 1 protein clay.</description>
		<jobString>Making protein clay.</jobString>
		<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_Machining</soundWorking>
		<allowMixingIngredients>true</allowMixingIngredients>
		<workAmount>250</workAmount>
		<targetCountAdjustment>5</targetCountAdjustment>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>MeatRaw</li>
					</categories>
				</filter>
				<count>20</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Chemfuel</li>
					</thingDefs>
				</filter>
				<count>5</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>MeatRaw</li>
			</categories>
		</fixedIngredientFilter>
		<products>
			<ProtienPutty>1</ProtienPutty>
		</products>
		<workSkill>Crafting</workSkill>
		<recipeUsers>
			<li>DrugLab</li>
		</recipeUsers>
		<researchPrerequisites>
			<li>ProtienRendering</li>
		</researchPrerequisites>
	</RecipeDef>
