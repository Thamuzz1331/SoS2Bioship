<Defs>
	<ThingDef ParentName="BuildingBase">
		<defName>Bio_Ship_Beam_Insensate</defName>
		<label>insensate shipflesh</label>
		<thingClass>Building</thingClass>
		<graphicData>
			<texPath>Things/Building/Linked/ShipBeam/BioShipBeam_Atlas</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<linkType>Basic</linkType>
			<linkFlags>
				<li>Wall</li>
				<li>Rock</li>
			</linkFlags>
			<damageData>
				<cornerTL>Damage/Corner</cornerTL>
				<cornerTR>Damage/Corner</cornerTR>
				<cornerBL>Damage/Corner</cornerBL>
				<cornerBR>Damage/Corner</cornerBR>
				<edgeTop>Damage/Edge</edgeTop>
				<edgeBot>Damage/Edge</edgeBot>
				<edgeLeft>Damage/Edge</edgeLeft>
				<edgeRight>Damage/Edge</edgeRight>
			</damageData>
		</graphicData>
		<altitudeLayer>Building</altitudeLayer>
		<passability>Impassable</passability>
		<blockWind>true</blockWind>
		<castEdgeShadows>true</castEdgeShadows>
		<fillPercent>1</fillPercent>
		<blockLight>true</blockLight>
		<canOverlapZones>false</canOverlapZones>
		<statBases>
			<MaxHitPoints>500</MaxHitPoints>
			<WorkToBuild>400</WorkToBuild>
			<Flammability>0</Flammability>
		</statBases>
		<description>The flesh of a a bioship.  Grown from the Shipheart, guided by protein scaffolds.\n\nThis has been nerve stapled and will not regenerate or spread.</description>
		<drawStyleCategory>Walls</drawStyleCategory>
		<comps>
			<li Class="SaveOurShip2.CompProps_ShipCachePart">
				<isHull>true</isHull>
				<hermetic>true</hermetic>
				<roof>true</roof>
			</li>
			<li>
				<compClass>CompColorable</compClass>
			</li>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTransmitter</compClass>
				<transmitsPower>true</transmitsPower>
			</li>
			<li Class="SaveOurShip2.CompProps_ShipHeat">
				<compClass>SaveOurShip2.CompShipHeat</compClass>
			</li>
		</comps>
		<placeWorkers>
		</placeWorkers>
		<rotatable>false</rotatable>
		<building>
			<shipPart>true</shipPart>
			<blueprintGraphicData>
				<texPath>Things/Building/Linked/Wall_Blueprint_Atlas</texPath>
			</blueprintGraphicData>
		</building>
		<costList>
		</costList>
		<terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
		<staticSunShadowHeight>1.0</staticSunShadowHeight>
		<researchPrerequisites>
		</researchPrerequisites>
		<designationHotKey>Misc1</designationHotKey>
		<holdsRoof>true</holdsRoof>
		<leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
		<killedLeavings>
		</killedLeavings>
	</ThingDef>
